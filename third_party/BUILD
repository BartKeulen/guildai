package(default_visibility = ["//visibility:public"])

# The libraries below can be used to express a dependency on a third
# party library that must be available at runtime. These are typically
# installed via 'pip install' but may be available in other ways.

py_library(
    name = "enum"
)

py_library(
    name = "pip"
)

py_library(
    name = "setuptools"
)

py_library(
    name = "tabview"
)

py_library(
    name = "tensorboard"
)

py_library(
    name = "tensorflow"
)

py_library(
    name = "twine"
)

py_library(
    name = "yaml"
)

py_library(
    name = "werkzeug"
)

# The aliases below are used to normalize references to third party
# libraries that are included with Guild.

alias(
    name = "click",
    actual = "@org_click",
)

alias(
    name = "psutil",
    actual = "@org_psutil",
)
